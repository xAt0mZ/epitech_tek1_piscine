#include "colle3.h"

int	last_line(int num_colle, int l, int h)
{
  my_putstr("[colle1-");
  my_putnbr(num_colle);
  my_putstr("] [");
  my_putnbr(l);
  my_putstr("] [");
  my_putnbr(h);
  my_putstr("]\n");
  return (0);
}

int	last_line_mult(int num_colle, int l, int h)
{
  while (num_colle <= 5)
    {
      my_putstr("[colle1-");
      my_putnbr(num_colle);
      my_putstr("] [");
      my_putnbr(l);
      my_putstr("] [");
      my_putnbr(h);
      my_putstr("]");
      num_colle++;
      my_putchar(' ');
    }
  my_putchar('\n');
  return (0);
}

int	false_glue()
{
  my_putstr("aucune");
  my_putchar('\n');
  return (0);
}
